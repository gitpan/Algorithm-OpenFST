T_FST
	{
	    if (sv_isobject($arg) && (SvTYPE(SvRV($arg)) == SVt_PVMG))
	        $var = ($type)SvIV((SV*)SvRV($arg));
	    else{
	        warn(\"${Package}::$func_name() -- $var is not a blessed SV\");
	        XSRETURN_UNDEF;
	    }
	}
T_PV
